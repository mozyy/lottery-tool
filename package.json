{
  "name": "lottery-tool",
  "version": "1.0.0",
  "private": true,
  "description": "抽签工具",
  "templateInfo": {
    "name": "taro-hooks",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production npm run dev:lottery:weapp",
    "dev:health:weapp": "npm run health npm run dev:weapp",
    "build:health:weapp": "npm run health npm run build:weapp",
    "dev:lottery:weapp": "npm run lottery npm run dev:weapp",
    "build:lottery:weapp": "npm run lottery npm run build:weapp",
    "build:weapp": "taro build --type weapp",
    "build:h5": "taro build --type h5",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "lottery": "cross-env TARO_APP_APP=lottery",
    "health": "cross-env TARO_APP_APP=health",
    "openapi": "./scripts/openapi.sh"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
